{
  "name": "pencil",
  "version": "1.0.0",
  "homepage": "https://pencil.yanke.ru",
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "classnames": "^2.3.1",
    "d3-geo": "^2.0.1",
    "lodash": "^4.17.21",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.16.0",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
