    "rxjs": "^6.6.7",
    "topojson-client": "^3.1.0"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --maxWorkers=1",
    "coverage": "npm t -- --coverage --watchAll=false",
    "deploy": "REACT_APP_API_URL=https://romanyanke.github.io/pencilbox-static npm run build && gh-pages -d build",
    "prettier": "prettier --config .prettierrc 'src/**/*.{ts,tsx}' --write",
