{
  "extends": ["tslint:recommended", "tslint-react-hooks", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts"]
  },

  "rules": {
    "no-console": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "no-unused-variable": [true, { "ignore-pattern": "^_" }]
  }
}
